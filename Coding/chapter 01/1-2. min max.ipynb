    "T = int(input())\n",
    "\n",
    "for test_case in range(1, T+1):\n",
    "  num = int(input())\n",
    "  number_list = list(map(int, input().split()))\n",
    "  max_num = max(number_list)\n",
    "  min_num = min(number_list)\n",
    "  answer = max_num - min_num\n",
    "  print(\"#%d\"%test_case, answer)"
