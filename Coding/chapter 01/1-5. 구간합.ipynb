   "source": [
    "TC = int(input())\n",
    "\n",
    "for tc in range(1, TC+1):\n",
    "    N, M = map(int, input().split())\n",
    "    num_list = list(map(int, input().split()))\n",
    "\n",
    "    sum_list = []\n",
    "    for i in range(N-M+1):\n",
    "        sum_list.append(sum(num_list[i:i+M]))\n",
    "\n",
    "    print('#%s %d'%(tc, max(sum_list)-min(sum_list)))"
   ]
  }
